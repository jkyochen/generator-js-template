{
  "name": "generator-js-template",
  "version": "0.0.6",
  "description": "Yeoman for JavaScript",
  "homepage": "",
  "author": {
    "name": "Lanlyhs"
  },
  "files": [
    "src/**/*"
  ],
  "main": "src/index.js",
  "scripts": {
    "format": "rome check --apply-suggested . && rome format --write .",
    "test": "jest",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "keywords": [
    "yeoman-generator",
    "formatter",
    "linter",
    "git-hooks"
  ],
  "dependencies": {
    "yeoman-generator": "^5.8.0"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "rome": "^11.0.0"
  },
  "lint-staged": {
    "*.{ts,js,json}": [
      "npm run format"
    ]
  },
  "repository": "lanlyhs/generator-js-template",
  "license": "Apache-2.0"
}
